structure       = gros___STATE__/__GRONAME__.gro      # __STATE__ and __GRONAME__ are reserved variables that will be replaced by lra.sh
ncpus           = -1                                  # to use all cores or specify a number of cpu cores available to your machine
sites_A         = all                                 # either specify the titrating residues (ex: '1N, 246, 292C')  or calculate on “all” residues
clean_pdb       = False                               # True if your pdb file requires a pre-processing procedure to be compatible with PypKa

## Output files
output            = pkas___STATE__/__GRONAME__.out                 # output pKa values will be ignored
titration_output  = titrations___STATE__/titration___GRONAME__.txt # optional, but required for LRA calculations

## Poisson-Boltzmann parameters
epsin           = 4 
ionicstr        = 0.1
pbc_dimensions  = 2

## Monte Carlo parameters
pH              = -5,15
